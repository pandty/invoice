<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org" >
<head>
	<th:block th:include="include :: header('添加商品编码')" />
</head>
<body class="white-bg">
	<div class="wrapper wrapper-content animated fadeInRight ibox-content">
		<form class="form-horizontal m" id="form-dict-add">
			<div class="form-group">
				<label class="col-sm-3 control-label is-required">机身编号或纳税人识别号：</label>
				<div class="col-sm-8">
					<input class="form-control" type="text" name="cxtj" id="cxtj" required>
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-3 control-label">查询方式：</label>
				<div class="col-sm-8">
					<select name="cxlx" class="form-control m-b">
					    <option value="1">纳税人识别号</option>
	                    <option value="2">机身编号</option>
	                </select>
				</div>
			</div>
		</form>
	</div>
	<th:block th:include="include :: footer" />
	<script type="text/javascript">
		var prefix = ctx + "shuzu/commoditycode";
	
		$("#form-dict-add").validate({
			rules:{
				dictSort:{
					digits:true
				},
			},
			focusCleanup: true
		});
		
		function submitHandler() {
	        if ($.validate.form()) {
	        	$.operate.save(prefix + "/add", $('#form-dict-add').serialize());
	        }
	    }
	</script>
</body>
</html>
